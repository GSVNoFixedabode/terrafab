@import 'leaflet.css'

body
  height: 100%
  width: 100%
  padding: 0
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  overflow: hidden
a
  color: #00B7FF

#map
  position: absolute
  top: 100px
  bottom: 0
  left: 0
  right: 0

  .moveable
    cursor: move

canvas
  z-index: 1
  position: absolute
  top: 100px
  left: 0
  pointer-events: none
