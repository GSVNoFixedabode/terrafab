body
  overflow: scroll !important
#modelPreviewPage
  text-align: center
  .progress
    position: absolute
    text-align: center
    width: 100%
    margin-top: -50px
    .spinner
      background-image: url('/images/ajax-loader.gif')
      height: 16px
      width: 16px
      background-size: 16px 16px
      display: inline-block
      margin-bottom: -3px
      margin-right: 8px
    p
      display: inline-block
      color: #666

  img.model
    max-width: 600px
    margin-top: 100px
  h1
    text-transform: uppercase
    font-size: 48px
  .shapewaysLogo
    background-image: url('http://static1.sw-cdn.net/rrstatic/img/sw-logo-dark-blue.svg')
    background-repeat: no-repeat
    width: 100%
    height: 40px
  .container
    max-width: 950px
    margin: 0 auto
    margin-bottom: 200px
    clear: both
    .col1, .col2
      float: left
      width: 50%
      margin-bottom: 200px