    "brfs": "0.0.7",
    "three": "git://github.com/bjoerge/threejs-node",
    "leaflet": "~0.6.4",
    "jquery": "git://github.com/bjoerge/jquery-node",
    "request": "*",
    "localStorage": "*",
    "events": "*",
    "http-get": "~0.5.10",
    "coffee-script": "~1.6.3",
    "canvas": "~1.0.4",
    "mkdirp": "~0.3.5",
    "rimraf": "~2.2.2",
